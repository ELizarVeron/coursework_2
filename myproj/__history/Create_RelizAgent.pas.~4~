unit Create_RelizAgent;

interface

uses
  Winapi.Windows, Winapi.Messages, System.SysUtils, System.Variants, System.Classes, Vcl.Graphics,
  Vcl.Controls, Vcl.Forms, Vcl.Dialogs, Vcl.StdCtrls, Vcl.WinXPickers, Data.Win.ADODB, Main_Class;

type
  TForm8 = class(TForm)
    Label1: TLabel;
    Label2: TLabel;
    Label3: TLabel;
    Label7: TLabel;
    Edit3: TEdit;
    DatePicker1: TDatePicker;
    ComboBox1: TComboBox;
    Button1: TButton;
    Button2: TButton;
  private
    { Private declarations }
  public
    { Public declarations }
     constructor Create(AOwner: TComponent); override;

  end;

var
  Form8: TForm8;

implementation
      constructor TForm8.Create(AOwner: TComponent);
      begin

             inherited;

              var mc:TMain_Class;
              mc:= TMain_Class.Create;
              var ado:TADOQuery;
              ado:=mc.sql_select('article', 'products','','',false);
              var i:integer;

               while not ado.Eof do
                  begin
                    ComboBox1.Items.Add(ado.FieldByName('article').AsString);
                    ado.Next
                  end;



      end;
{$R *.dfm}

end.
