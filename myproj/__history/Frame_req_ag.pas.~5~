unit Frame_req_ag;

interface

uses
  Winapi.Windows, Winapi.Messages, System.SysUtils, System.Variants, System.Classes,
  Vcl.Graphics, Vcl.Controls, Vcl.Forms, Vcl.Dialogs, Vcl.StdCtrls, Vcl.ExtCtrls,  Request_Agents_Class ,Request_Info, TABLE_Requests_agent;

type
  TFrame9 = class(TFrame)
    Panel1: TPanel;
    Label2: TLabel;
    Label3: TLabel;
    Label4: TLabel;
    Label5: TLabel;
    Label6: TLabel;
    Button1: TButton;
    Label1: TLabel;
    Label7: TLabel;
    Label8: TLabel;
    procedure Button1Click(Sender: TObject);
  private
    { Private declarations }
  public
    var Req_on_frame:  TRequest_agent;
  end;

implementation

{$R *.dfm}

procedure TFrame9.Button1Click(Sender: TObject);
begin
      var WINFO: TForm11;
      WINFO:= TForm11.Create(nil);
      WINFO.Label2.Caption:= DateToStr( Req_on_frame.Date_Of_Create);
      WINFO.Label4.Caption:=Req_on_frame.Company;
      WINFO.Label6.Caption:=Req_on_frame.Status;
      WINFO.Label8.Caption:=Req_on_frame.Premayment.ToString;

      WINFO.Show;


end;

end.
